        {
          "queryText": "SELECT \n  u.username AS \"Usuario\",\n  q.title AS \"Pregunta\",\n  -- Edad calculada\n  (strftime('%Y', 'now') - strftime('%Y', u.date_of_birth)) AS \"Edad\",\n  -- Fecha formateada\n  strftime('%H:%M - %d/%m', q.date_time) AS \"Fecha\",\n  -- Modelo de IA en mayúsculas\n  UPPER(q.model_llm) AS \"Modelo IA\",\n  -- Icono de satisfacción\n  CASE WHEN q.\"like\" = 1 THEN '✅ OK' ELSE '❌ No' END AS \"Rating\",\n  -- Tiempo de respuesta\n  ROUND(q.time_out, 2) || 's' AS \"Latencia\",\n  -- Tokens consumidos\n  q.used_tokens AS \"Tokens\"\nFROM \"user\" u\nJOIN \"question\" q ON u.id = q.user_id\nORDER BY q.id DESC\nLIMIT 5;",
          "queryType": "table",
          "rawQueryText": "SELECT \n  u.username AS \"Usuario\",\n  q.title AS \"Pregunta\",\n  -- Edad calculada\n  (strftime('%Y', 'now') - strftime('%Y', u.date_of_birth)) AS \"Edad\",\n  -- Fecha formateada\n  strftime('%H:%M - %d/%m', q.date_time) AS \"Fecha\",\n  -- Modelo de IA en mayúsculas\n  UPPER(q.model_llm) AS \"Modelo IA\",\n  -- Icono de satisfacción\n  CASE WHEN q.\"like\" = 1 THEN '✅ OK' ELSE '❌ No' END AS \"Rating\",\n  -- Tiempo de respuesta\n  ROUND(q.time_out, 2) || 's' AS \"Latencia\",\n  -- Tokens consumidos\n  q.used_tokens AS \"Tokens\"\nFROM \"user\" u\nJOIN \"question\" q ON u.id = q.user_id\nORDER BY q.id DESC\nLIMIT 5;",
          "refId": "A",
          "timeColumns": [
            "time",
            "ts"
          ]
        }
